    "singleAttributePerLine": false,
    "tabWidth": 4,
    "printWidth": 120,
    "plugins": [
        "prettier-plugin-tailwindcss",
        "prettier-plugin-blade"
    ],
    "overrides": [
        {
            "files": [
                "*.blade.php"
            ],
            "options": {
                "parser": "blade"
            }
        }
    ]
}
